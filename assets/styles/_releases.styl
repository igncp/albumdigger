.back
  margin 30px 0px

.list-five
  margin-bottom 40px

.singleRelease, #releases
  border: 1px solid #CCC
  border-radius(12px)
  box-shadow(0px 0px 10px .1px #ddd)
  display: none
  margin-top 20px
  padding: 10px

.singleRelease
  .notes
    margin-top 20px

  .title
    font-weight bold

.toggle-charts
  position absolute
  right 60px

#releases
  form
    margin-bottom 20px

    input[type="radio"]
      margin-left 20px
    input[type="radio"]:first-child
      margin-left 0px
      
  .release-row
    margin 10px 0px

    a { text-decoration: none; }